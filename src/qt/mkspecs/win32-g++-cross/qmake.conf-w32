#
# $Id: qmake.conf,v 1.1.2.1 2005/03/23 16:46:44 habacker Exp $
#
# qmake configuration for win32-x-g++ (crosscompiler)
#
# Written for MinGW cross compiler 2.0.0.
#

MAKEFILE_GENERATOR	= MINGW
TEMPLATE		= app
CONFIG			+= qt warn_on release link_prl
DEFINES			+= _REENTRANT UNICODE QT_LARGEFILE_SUPPORT QT_SHARED

QMAKE_CC		= $(CROSS)gcc
QMAKE_LEX		= flex
QMAKE_YACC		= byacc
QMAKE_YACCFLAGS		= -d
QMAKE_CFLAGS_DEPS	= -M
QMAKE_CFLAGS_WARN_ON	= -w
QMAKE_CFLAGS_WARN_OFF	= -w
QMAKE_CFLAGS_RELEASE	= -O2 -fpermissive
QMAKE_CFLAGS_DEBUG	= -g -gstabs -fpermissive
QMAKE_CFLAGS_YACC	= -Wno-unused -Wno-parentheses

QMAKE_CXX		= $(CROSS)g++
QMAKE_CXXFLAGS		= $$QMAKE_CFLAGS
QMAKE_CXXFLAGS_DEPS	= $$QMAKE_CFLAGS_DEPS
QMAKE_CXXFLAGS_WARN_ON	= $$QMAKE_CFLAGS_WARN_ON
QMAKE_CXXFLAGS_WARN_OFF	= $$QMAKE_CFLAGS_WARN_OFF
QMAKE_CXXFLAGS_RELEASE	= $$QMAKE_CFLAGS_RELEASE
QMAKE_CXXFLAGS_DEBUG	= $$QMAKE_CFLAGS_DEBUG
QMAKE_CXXFLAGS_YACC	= $$QMAKE_CFLAGS_YACC
QMAKE_CXXFLAGS_THREAD	= $$QMAKE_CFLAGS_THREAD
QMAKE_CXXFLAGS_RTTI_ON	= $$QMAKE_CFLAGS_RTTI_ON
QMAKE_CXXFLAGS_RTTI_OFF	= $$QMAKE_CFLAGS_RTTI_OFF
QMAKE_CXXFLAGS_EXCEPTIONS_ON = $$QMAKE_CFLAGS_EXCEPTIONS_ON
QMAKE_CXXFLAGS_EXCEPTIONS_OFF = $$QMAKE_CFLAGS_EXCEPTIONS_OFF

QMAKE_INCDIR_QT		= $(QTDIR)/include
QMAKE_LIBDIR_QT		= $(QTDIR)/lib

QMAKE_RUN_CC		= $(CC) -c $(CFLAGS) $(INCPATH) -o $obj $src
QMAKE_RUN_CC_IMP	= $(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<
QMAKE_RUN_CXX		= $(CXX) -c $(CXXFLAGS) $(INCPATH) -o $obj $src
QMAKE_RUN_CXX_IMP	= $(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

QMAKE_LINK		= $(CROSS)g++
QMAKE_LFLAGS		= -Wl,--enable-stdcall-fixup -Wl,--enable-auto-import -Wl,--enable-runtime-pseudo-reloc
QMAKE_LFLAGS_RELEASE	= -Wl,-s
QMAKE_LFLAGS_DEBUG      =
QMAKE_LFLAGS_CONSOLE	= -Wl,--subsystem,console
QMAKE_LFLAGS_WINDOWS	= -Wl,--subsystem,windows -e __Z19QtWinMainCRTStartupv
QMAKE_LFLAGS_CONSOLE_DLL= -Wl,--subsystem,console -shared
QMAKE_LFLAGS_WINDOWS_DLL= -Wl,--subsystem,windows -shared
QMAKE_LINK_OBJECT_MAX	= 10
QMAKE_LINK_OBJECT_SCRIPT= object_ld_script
QMAKE_LINK_OBJMOC_SCRIPT= objmoc_ld_script

QMAKE_LIBS_CONSOLE	=
QMAKE_LIBS_WINDOWS	= -lpthreadAQ -lkernel32 -luser32 -lgdi32 -lcomdlg32 -ladvapi32 -lshell32 -lole32 -loleaut32 -luuid -limm32 -lwinmm -lws2_32 -lwinspool
QMAKE_LIBS_QT		= -lqt
QMAKE_LIBS_QT_THREAD	= -lqt-mt
QMAKE_LIBS_THREAD       = -lpthreadAQ
QMAKE_LIBS_QT_ENTRY     = -lqtmain -lmingw32
QMAKE_LIBS_OPENGL	= -lopengl32 -lglu32

# those must point to the ones from the linux distribution
QMAKE_MOC		= moc
QMAKE_UIC		= uic
# not available on linux
# QMAKE_IDC		= $(QTDIR)/bin/idc.exe

QMAKE_IDL		= midl
QMAKE_LIB		= $(CROSS)ar -ru
QMAKE_RC		= $(CROSS)windres

QMAKE_ZIP		= zip -r -9

QMAKE_COPY		= cp -f
QMAKE_MOVE		= mv -f
QMAKE_DEL_FILE		= rm -f
QMAKE_DEL_DIR		= rmdir
QMAKE_STRIP		= $(CROSS)strip
QMAKE_STRIPFLAGS_LIB 	+= --strip-unneeded
QMAKE_CHK_DIR_EXISTS	= test -d
QMAKE_MKDIR		= mkdir -p

